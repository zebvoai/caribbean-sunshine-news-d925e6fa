{
  "rewrites": [
    {
      "source": "/api/mongo-api",
      "destination": "https://xqzffikjyrdmbuptreyj.supabase.co/functions/v1/mongo-api"
    },
    {
      "source": "/api/storage/:bucket/:path*",
      "destination": "https://xqzffikjyrdmbuptreyj.supabase.co/storage/v1/object/public/:bucket/:path*"
    },
    {
      "source": "/api/auth/token",
      "destination": "https://xqzffikjyrdmbuptreyj.supabase.co/auth/v1/token"
    },
    {
      "source": "/news/:slug",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i)whatsapp|facebookexternalhit|facebot|twitterbot|telegrambot|slackbot|discordbot|linkedinbot|pinterest|googlebot|bingbot|yandex|baiduspider|applebot|bot|crawl|spider|preview"
        }
      ],
      "destination": "https://xqzffikjyrdmbuptreyj.supabase.co/functions/v1/og-proxy?slug=:slug"
    },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
